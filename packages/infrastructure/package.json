{
  "private": true,
  "name": "@serverless-aws-cdk-ecommerce/infrastructure",
  "version": "1.0.0",
  "description": "Serverless AWS-CDK Infrastructure as Code",
  "scripts": {
    "clean": "rimraf cdk.out",
    "deploy:backends": "dotenv -- cdk deploy --json --require-approval never --app deploy-backends.js",
    "deploy:frontends": "dotenv -- cdk deploy --require-approval never --app deploy-frontends.js",
    "destroy:backends": "dotenv -- cdk destroy --require-approval never --app deploy-backends.js",
    "destroy:frontends": "dotenv -- cdk destroy --require-approval never --app deploy-frontends.js",
    "format": "prettier --write \"**/*.{js,jsx}\""
  },
  "devDependencies": {
    "prettier": "1.18.2",
    "rimraf": "3.0.0"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "1.12.0",
    "@aws-cdk/aws-cognito": "1.12.0",
    "@aws-cdk/aws-dynamodb": "1.12.0",
    "@aws-cdk/aws-iam": "1.12.0",
    "@aws-cdk/aws-lambda": "1.12.0",
    "@aws-cdk/aws-route53": "1.12.0",
    "@aws-cdk/aws-route53-targets": "1.12.0",
    "@aws-cdk/aws-s3": "1.12.0",
    "@aws-cdk/aws-s3-deployment": "1.11.0",
    "@aws-cdk/aws-ssm": "1.12.0",
    "@aws-cdk/core": "1.12.0",
    "aws-cdk": "1.12.0",
    "dotenv-cli": "2.0.1",
    "json-dynamo-putrequest": "1.0.0"
  }
}
